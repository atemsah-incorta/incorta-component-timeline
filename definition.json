{
  "icon": "./assets/icon.svg",
  "name": "Timeline",
  "hint": "Drag any 2 time based fields to the Start/End trays and a field marking them to the label tray then enjoy visualizing your data on the timeline as floating bars",
  "options": {
    "individualFilter": true
  },
  "settings": [
    {
      "name": "Visualization",
      "settings": [
        {
          "name": "Mode",
          "key": "mode",
          "type": "dropdown",
          "defaultValue": "default",
          "options": [
            { "key": "default", "label": "Default" },
            { "key": "gantt", "label": "Gantt"}
          ]
        },
        {
          "name": "Time Scale Unit",
          "key": "unit",
          "type": "dropdown",
          "defaultValue": "default",
          "options": [
            { "key": "default", "label": "Default" },
            { "key": "millisecond", "label": "Millisecond" },
            { "key": "second", "label": "Second" },
            { "key": "minute", "label": "Minute" },
            { "key": "hour", "label": "Hour" },
            { "key": "day", "label": "Day" },
            { "key": "week", "label": "Week" },
            { "key": "month", "label": "Month" },
            { "key": "quarter", "label": "Quarter" },
            { "key": "year", "label": "Year" }
          ]
        },
        {
          "name": "Time Scale Format",
          "key": "format",
          "type": "text"
        }
      ]
    }
  ],
  "bindingsTrays": [
    {
      "key": "tray-start",
      "name": "Start",
      "queryRole": "measure",
      "minCount": 1,
      "maxCount": 1,
      "settings": [
        {
          "name": "Format",
          "settings": [
            { "key": "format", "name": "Format", "type": "date-format" }
          ]
        }
      ]
    },
    {
      "key": "tray-end",
      "name": "End",
      "queryRole": "measure",
      "minCount": 1,
      "maxCount": 1,
      "settings": [
        {
          "name": "Format",
          "settings": [
            { "key": "format", "name": "Format", "type": "date-format" }
          ]
        }
      ]
    },
    {
      "key": "tray-label",
      "name": "Label",
      "queryRole": "measure",
      "minCount": 1,
      "maxCount": 1
    },
    {
      "key": "tray-tooltip",
      "name": "Tooltip",
      "queryRole": "measure"
    }
  ]
}
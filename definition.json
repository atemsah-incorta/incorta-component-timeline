{
  "icon": "./assets/icon.svg",
  "name": "Timeline",
  "hint": "Place 1 time based column in Start, 1 time based column in End and 1 column in Category",
  "options": {
    "individualFilter": true
  },
  "settings": [
    {
      "name": "Visualization",
      "settings": [
        {
          "name": "Mode",
          "key": "mode",
          "type": "dropdown",
          "defaultValue": "default",
          "options": [
            { "key": "default", "label": "Default" },
            { "key": "gantt", "label": "Gantt"}
          ]
        },
        {
          "name": "Time Scale Unit",
          "key": "unit",
          "type": "dropdown",
          "defaultValue": "default",
          "options": [
            { "key": "default", "label": "Default" },
            { "key": "millisecond", "label": "Millisecond" },
            { "key": "second", "label": "Second" },
            { "key": "minute", "label": "Minute" },
            { "key": "hour", "label": "Hour" },
            { "key": "day", "label": "Day" },
            { "key": "week", "label": "Week" },
            { "key": "month", "label": "Month" },
            { "key": "quarter", "label": "Quarter" },
            { "key": "year", "label": "Year" }
          ]
        },
        {
          "name": "Time Scale Format",
          "key": "format",
          "type": "text"
        }
      ]
    },
    {
      "name": "Legend",
      "settings": [
        {
          "name": "Enabled",
          "key": "legendEnabled",
          "type": "boolean",
          "defaultValue": true
        },
        {
          "name": "Position",
          "key": "legendPosition",
          "type": "dropdown",
          "defaultValue": "top",
          "options": [
            { "key": "top", "label": "Top" },
            { "key": "bottom", "label": "Bottom" },
            { "key": "left", "label": "Left" },
            { "key": "right", "label": "Right" }
          ]
        }
      ]
    }
  ],
  "bindingsTrays": [
    {
      "key": "tray-start",
      "name": "Start",
      "queryRole": "measure",
      "minCount": 1,
      "maxCount": 1,
      "settings": [
        {
          "name": "Format",
          "settings": [
            { "key": "format", "name": "Format", "type": "date-format" }
          ]
        }
      ]
    },
    {
      "key": "tray-end",
      "name": "End",
      "queryRole": "measure",
      "minCount": 1,
      "maxCount": 1,
      "settings": [
        {
          "name": "Format",
          "settings": [
            { "key": "format", "name": "Format", "type": "date-format" }
          ]
        }
      ]
    },
    {
      "key": "tray-label",
      "name": "Category",
      "queryRole": "measure",
      "minCount": 1,
      "maxCount": 1
    },
    {
      "key": "tray-color",
      "name": "Color By",
      "queryRole": "measure",
      "maxCount": 1
    },
    {
      "key": "tray-tooltip",
      "name": "Tooltip",
      "queryRole": "measure"
    }
  ]
}